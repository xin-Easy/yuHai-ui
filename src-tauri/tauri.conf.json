{
  "$schema": "https://schema.tauri.app/config/2",
  "version": "0.0.0",
  "app": {
    "security": {
      "capabilities": [
        "default"
      ],
      "csp": null
    },
    "windows": [
      {
        "height": 800,
        "label": "main",
        "title": "蒲公英引擎",
        "width": 1200
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "pnpm build:web",
    "beforeDevCommand": "pnpm dev:web",
    "devUrl": "http://localhost:3006",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/128x128.png",
      "icons/icon.png",
      "icons/icon.ico"
    ],
    "resources": [
      "resources/yuHai-core.zip"
    ],
    "windows": {
      "wix": {
        "language": "zh-CN"
      },
      "nsis": {
        "installerIcon": "icons/icon.ico"
      }
    },
    "targets": [
      "deb",
      "rpm",
      "appimage",
      "nsis",
      "msi"
    ]
  },
  "identifier": "yuHai-ui",
  "plugins": {
    "updater": {
      "endpoints": [
        "https://xin-easy.github.io/yuHai-ui/latest.json"
      ],
